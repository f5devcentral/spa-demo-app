{
  "name": "spa",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "vite --port 8081",
    "start-dark": "VITE_APP_GLOBAL_COLOR=#FFF VITE_APP_GLOBAL_BACKGROUND_COLOR=#000 vite --port 8081",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview --port 8081",
    "test": "vitest",
    "test-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.28.3",
    "axios": "^0.27.2",
    "element-plus": "^2.2.17",
    "utility-types": "^3.10.0",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.5.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.13"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "@vue/eslint-config-typescript"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "double"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
