{
  "name": "spa",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 8081",
    "dev:dark": "VITE_APP_GLOBAL_COLOR=#FFF VITE_APP_GLOBAL_BACKGROUND_COLOR=#000 vite --port 8081",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 8081",
    "test:unit": "vitest --environment jsdom",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test preview http://localhost:8081/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:8081/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.36.0",
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.4.0",
    "clipboard-copy": "^4.0.1",
    "element-plus": "^2.3.6",
    "utility-types": "^3.10.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.1",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-istanbul": "^0.31.4",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "axios-mock-adapter": "^1.21.4",
    "cypress": "^12.16.0",
    "eslint": "^8.42.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.13.0",
    "jsdom": "^21.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "typescript": "~5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.2",
    "vitest-localstorage-mock": "^0.0.1",
    "vue-tsc": "^1.8.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "@vue/eslint-config-typescript"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "double"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
