{
  "name": "spa",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 8081",
    "dev:dark": "VITE_APP_GLOBAL_COLOR=#FFF VITE_APP_GLOBAL_BACKGROUND_COLOR=#000 vite --port 8081",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 8081",
    "test:unit": "vitest --environment jsdom",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test preview http://localhost:8081/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:8081/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.29.0",
    "axios": "^1.1.3",
    "clipboard-copy": "^4.0.1",
    "element-plus": "^2.2.19",
    "utility-types": "^3.10.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "18.11.4",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.24.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.1",
    "@vue/tsconfig": "^0.1.3",
    "axios-mock-adapter": "^1.21.2",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.6.0",
    "jsdom": "^20.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vitest": "^0.24.3",
    "vitest-localstorage-mock": "^0.0.1",
    "vue-tsc": "^1.0.9"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "@vue/eslint-config-typescript"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "double"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
